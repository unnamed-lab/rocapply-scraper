{
  "name": "rocapply-scraper",
  "version": "1.0.0",
  "type": "module",
  "description": "Rocapply Web Scraper is a TypeScript-based tool designed to automate the extraction of data from the Rocapply website. It provides a robust and lightweight API, built on Express, for efficient web scraping and data retrieval.",
  "main": "index.js",
  "scripts": {
    "start": "tsx src/server.ts",
    "build": "tsc",
    "serve": "node dist/server.js",
    "dev": "nodemon src/server.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test/**",
      "**/docs/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unnamed-lab/rocapply-scraper.git"
  },
  "keywords": [
    "rocapply",
    "web",
    "scraper",
    "web",
    "crawler"
  ],
  "author": "Adebayo Anuoluwa Success",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unnamed-lab/rocapply-scraper/issues"
  },
  "homepage": "https://github.com/unnamed-lab/rocapply-scraper#readme",
  "dependencies": {
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "marked": "^15.0.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^16.11.12",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.2",
    "typescript": "^4.5.4"
  }
}
